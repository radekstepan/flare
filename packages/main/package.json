{
  "dependencies": {
    "dot-prop": "sindresorhus/dot-prop",
    "jexl": "^2.3.0"
  },
  "devDependencies": {
    "@radekstepan/flare-types": "^1.0.0",
    "@radekstepan/flare-utils": "^1.0.0",
    "@types/jexl": "^2.3.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.2.5",
    "@types/sinon": "^10.0.15",
    "ava": "^5.3.0",
    "c8": "^7.14.0",
    "del-cli": "^5.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "sinon": "^15.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "test": "tsc --noEmit && NODE_NO_WARNINGS=1 c8 ava",
    "test:dev": "clear ; tsc --noEmit && NODE_NO_WARNINGS=1 ava --serial --fail-fast",
    "build": "del-cli dist && tsc"
  },
  "name": "@radekstepan/flare",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "files": [
    "dist/"
  ],
  "type": "module",
  "author": "Radek Stepan <dev@radekstepan.com>",
  "license": "MIT",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "ava": {
    "files": [
      "test/**/*.test.*"
    ],
    "timeout": "1m",
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "c8": {
    "reporter": [
      "html",
      "text"
    ]
  }
}
